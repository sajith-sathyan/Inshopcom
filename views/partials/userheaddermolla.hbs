<header class="header">
    <div class="header-top">
        <div class="container">
            <div class="header-left">



            </div><!-- End .header-left -->


        </div><!-- End .container -->
    </div><!-- End .header-top -->

    <div class="header-middle sticky-header">
        <div class="container">
            <div class="header-left">
                <button class="mobile-menu-toggler">
                    <span class="sr-only">Toggle mobile menu</span>
                    <i class="icon-bars"></i>
                </button>

                <a href="/" class="logo">
                    <span style="font-weight: 800;font-size:larger;color:#cc3333">INSHOPCOM</span>
                </a>

                <nav class="main-nav">
                    <ul class="menu sf-arrows">
                        <li>
                            <a href="/">Home</a>


                        </li>
                        <li>
                            <a href="view-Order">My Order</a>



                        </li>
                        <li>
                            <a href="My-Account">My-Account</a>



                        </li>


                    </ul><!-- End .menu -->
                </nav><!-- End .main-nav -->
            </div><!-- End .header-left -->

            <div class="header-right">

                {{#if user}}
                <div class="dropdown cart-dropdown">
                    <a href="wishlist" class="dropdown-toggle" role="button" aria-expanded="false"
                        data-display="static">

                        <i class="icon-heart"></i>

                        <span id="cart-count" class="cart-count"></span>
                    </a>
                    {{else}}
                    <div></div>
                    {{/if}}


                </div><!-- End .cart-dropdown -->
                {{#if user}}
                <a href="cart">
                    <div class="dropdown cart-dropdown">
                        <a class="dropdown-toggle" role="button" data-display="static">

                            <i class="icon-shopping-cart"></i>

                            <span id="cart-count" class="cart-count">{{cartCount}}</span>
                        </a>
                        {{else}}
                        <div></div>
                        {{/if}}

                    </div><!-- End .cart-dropdown -->
                </a>
                <div class="dropdown-menu dropdown-menu-right" style="padding-left: 10px;"></div>
                <div type="button" class="btn btn-primary " data-bs-toggle="dropdown" aria-expanded="false">
                    {{#if user}}
                    {{user.name}}
                    {{else}}
                    Account
                    {{/if}}
                </div>
            </div>
        </div><!-- End .header-right -->
    </div><!-- End .container -->
    </div><!-- End .header-middle -->
</header><!-- End .header -->