    {{!-- sawl --}}
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
        function addToCart(proId) {
            console.log("inside ajax js");
            $.ajax({
                url: "/add-to-cart/" + proId,
                method: "get",
                success: (response) => {
                    if (response.status) {
                        let count = $('#cart-count').html()
                        count = parseInt(count) + 1
                        $('#cart-count').html(count)
                        Swal.fire({

                            icon: 'success',
                            title: 'This in your cart',
                            showConfirmButton: false,
                            timer: 1500
                        })

                    }

                }
            })
        }
    </script>

    <script>
        function AddWishList(proId) {
            console.log("loggd")
            $.ajax({
                url: "/addWishlist/" + proId,
                method: "get",
                data: {
                    proId
                },
                success: (response) => {
                    alert("added")
                }
            })
        }

    </script>
