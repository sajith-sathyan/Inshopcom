            confirmPassword
            let password = document.getElementById("password").value
            let passwordRepeat = document.getElementById("confirmPassword").value
            $.ajax({

                url: "/change-pwd-repeatpassword",
                method: "post",
                data: {
                    password,
                    passwordRepeat
                },
                success: (response) => {
                   Swal.fire({
                            icon: 'success',
                            title: 'success',
                            text: "rder placed succesfully",
                            showCancelButton: true,
                            confirmButtonColor: "#3085d6",
                            cancelButtonColor: "#3085d6",
                            confirmButtonText: "Go To Home",
                            cancelButtonText: "Go to My Account"
                        }).then((result) => {
                            if (result.isConfirmed) {
                                location.href = "/"
                            } else {
                                location.href = "/My-Account"
                            }
                        })

                        
                    

                }
            })
        })
